### Galaxy Image Creation Examples for SpaceBackGroundBuilder API

### 1. Basic Spiral Galaxy
POST http://localhost:8080/images/create
Content-Type: application/json

{
  "name": "Basic Spiral Galaxy",
  "description": "Simple spiral galaxy with core and arms",
  "forceUpdate": false,
  "sizeCmd": {
    "height": 2000,
    "width": 2000,
    "seed": 12345,
    "octaves": 1,
    "persistence": 0.5,
    "lacunarity": 2.0,
    "scale": 100.0,
    "preset": "CUSTOM",
    "useMultiLayer": true,
    "noiseType": "FBM",
    "layers": [
      {
        "name": "background",
        "enabled": true,
        "octaves": 2,
        "persistence": 0.3,
        "lacunarity": 2.0,
        "scale": 250.0,
        "opacity": 1.0,
        "blendMode": "NORMAL",
        "noiseType": "FBM",
        "seedOffset": 0
      },
      {
        "name": "galaxy_core",
        "enabled": true,
        "octaves": 4,
        "persistence": 0.6,
        "lacunarity": 2.1,
        "scale": 120.0,
        "opacity": 0.8,
        "blendMode": "ADD",
        "noiseType": "FBM",
        "seedOffset": 1000
      },
      {
        "name": "spiral_arms",
        "enabled": true,
        "octaves": 5,
        "persistence": 0.55,
        "lacunarity": 2.3,
        "scale": 90.0,
        "opacity": 0.7,
        "blendMode": "OVERLAY",
        "noiseType": "FBM",
        "seedOffset": 2000
      }
    ]
  },
  "colorCmd": {
    "back": "#000000",
    "middle": "#1a004a",
    "fore": "#8a2be2",
    "backThreshold": 0.25,
    "middleThreshold": 0.75,
    "interpolationType": "SMOOTHSTEP"
  }
}

