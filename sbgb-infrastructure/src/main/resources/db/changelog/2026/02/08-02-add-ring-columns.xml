<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.24.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1707400000001-1" author="daniel">
        <addColumn tableName="galaxy_image">
            <column name="ring_radius" type="DOUBLE">
                <constraints nullable="true"/>
            </column>
            <column name="ring_width" type="DOUBLE">
                <constraints nullable="true"/>
            </column>
            <column name="ring_intensity" type="DOUBLE">
                <constraints nullable="true"/>
            </column>
            <column name="core_to_ring_ratio" type="DOUBLE">
                <constraints nullable="true"/>
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>
