<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.24.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="20260203-01-add-fractal-params" author="junie">
        <addColumn tableName="noise_image">
            <column name="octaves" type="INT" defaultValueNumeric="1">
                <constraints nullable="false"/>
            </column>
            <column name="persistence" type="DOUBLE" defaultValueNumeric="0.5">
                <constraints nullable="false"/>
            </column>
            <column name="lacunarity" type="DOUBLE" defaultValueNumeric="2.0">
                <constraints nullable="false"/>
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>