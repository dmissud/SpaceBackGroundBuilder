<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.24.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1707315600000-2" author="daniel">
        <addColumn tableName="galaxy_image">
            <column name="galaxy_type" type="VARCHAR(30)" defaultValue="SPIRAL">
                <constraints nullable="false"/>
            </column>
            <column name="cluster_count" type="INT">
                <constraints nullable="true"/>
            </column>
            <column name="cluster_size" type="DOUBLE">
                <constraints nullable="true"/>
            </column>
            <column name="cluster_concentration" type="DOUBLE">
                <constraints nullable="true"/>
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>
